@model Cf.Data.ExceptionalAmount
<h1>@ViewBag.Action @ViewBag.ModuleNameExceptionalAmount</h1>


<h4>@ViewBag.ConfirmDelete</h4>
<div>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="@ViewBag.Yes" class="btn btn-danger" />
            <input type="button" onclick="location.href='@Url.Action("Index")'" value="@ViewBag.No" class="btn btn-default" />
        </div>
    }
</div>
