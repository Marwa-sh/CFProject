<div class="row">
    <div class="col-xs-12">
        <div class="card">
            <div class="card-header">
                <h4 class="card-title">@ViewBag.GrantVwTitle @if(Model.GrantVwViewModel.List.Count == 0) {<span class="tag tag-default">@Model.GrantVwViewModel.List.Count</span>} else {<span class="tag tag-danger">@Model.GrantVwViewModel.List.Count</span> }</h4>
                <a class="heading-elements-toggle"><i class="icon-ellipsis font-medium-3"></i></a>
                <div class="heading-elements">
                    <ul class="list-inline mb-0">
                        <li><a data-action="collapse"><i class="icon-plus4"></i></a></li>
                        <li><a data-action="reload"><i class="icon-reload"></i></a></li>
                        <li><a data-action="expand"><i class="icon-expand2"></i></a></li>
                    </ul>
                </div>
            </div>
            <div class="card-body collapse out">
                <div class="card-block card-dashboard">
                    <button class="btn btn-success" type="button">
                        @Html.ActionLink((string)ViewBag.Insert + " " + (string)ViewBag.GrantVwTitle, "Create", "Grant")
                    </button>
                </div>
                <div class="table-responsive">
                    <table class="table table-bordered mb-0">
                        <thead class="thead-inverse">
					        <tr>
					            <th>
					                @Html.DisplayNameFor(model => model.GrantVwViewModel.Instance.GrantDecisionNumber)
					            </th>
					            <th>
					                @Html.DisplayNameFor(model => model.GrantVwViewModel.Instance.GrantDecisionYear)
					            </th>
					            <th>
					                @Html.DisplayNameFor(model => model.GrantVwViewModel.Instance.GrantDecisionDate)
					            </th>
					            <th>
					                @Html.DisplayNameFor(model => model.GrantVwViewModel.Instance.Amount)
					            </th>
           
					            <th></th>
					        </tr>
                        </thead>
                        <tbody>
        @foreach (var item in Model.GrantVwViewModel.List)
        {
				            <tr>
                				<td>
                    				 @Html.DisplayFor(modelItem => item.GrantDecisionNumber)
                				</td>
                				<td>
                    				 @Html.DisplayFor(modelItem => item.GrantDecisionYear)
                				</td>
                				<td>
                    				 @Html.DisplayFor(modelItem => item.GrantDecisionDate)
                				</td>
                				<td>
                    				 @Html.DisplayFor(modelItem => item.Amount)
                				</td>
				                <td>
									<div class="btn-group btn-group-sm" role="group" aria-label="Basic example">
										<button type="button" class="btn btn-info">
					                    	@Html.ActionLink((string)ViewBag.Details, "Details", "Grant",
                        					new
											{
												product = item.ProductId
                        					}, null)
                                        </button>
                                        <button type="button" class="btn btn-warning">
					                    	@Html.ActionLink((string)ViewBag.Update, "Edit", "Grant",
                        					new
                        					{
												product = item.ProductId
                        					}, null)
                                        </button>
                                        <button type="button" class="btn btn-danger deleteLink" data-original-title="@ViewBag.Delete" id="delete" data-id="@item.ProductId" data-url="@Url.Action("Delete", "Grant")">@ViewBag.Delete</button>
                                    </div>
                                </td>
                            </tr>
				        }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
