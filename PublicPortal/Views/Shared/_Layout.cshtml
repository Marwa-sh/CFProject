<!DOCTYPE html>
<html lang="en" data-textdirection="rtl">


<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title  الصندوق التعاوني بدمشق</title>
    <!--
        Change the Style of button OK for sweet alert library
        so that the button will be at the center
        -->

    <style>
        .swal-button-container {
            display: flex;
            align-items: center;
            justify-content: center;
        }
    </style>





    <!-- BEGIN VENDOR CSS-->
    <link rel="stylesheet" type="text/css" href="~/Content/assets/css-rtl/bootstrap.css">
    <link href="~/Content/assets/bootstrap-datetimepicker.min.css" rel="stylesheet" />
    <!-- font icons-->
    <link rel="stylesheet" type="text/css" href="~/Content/assets/fonts/icomoon.css">
    <link rel="stylesheet" type="text/css" href="~/Content/assets/fonts/flag-icon-css/css/flag-icon.min.css">
    <!-- END VENDOR CSS-->
    <!-- BEGIN ROBUST CSS-->
    <link rel="stylesheet" type="text/css" href="~/Content/assets/css-rtl/bootstrap-extended.css">
    <link rel="stylesheet" type="text/css" href="~/Content/assets/css-rtl/app.css">
    <link rel="stylesheet" type="text/css" href="~/Content/assets/css-rtl/colors.css">
    <link rel="stylesheet" type="text/css" href="~/Content/assets/css-rtl/custom-rtl.css">
    <!-- END ROBUST CSS-->
    <!-- BEGIN Page Level CSS-->
    <link rel="stylesheet" type="text/css" href="~/Content/assets/css-rtl/core/menu/menu-types/vertical-menu.css">
    <link rel="stylesheet" type="text/css" href="~/Content/assets/css-rtl/core/menu/menu-types/vertical-overlay-menu.css">
    <link rel="stylesheet" type="text/css" href="~/Content/assets/css-rtl/core/colors/palette-gradient.css">
    <!-- END Page Level CSS-->
    <!-- BEGIN Custom CSS-->
    <link rel="stylesheet" type="text/css" href="~/Content/assets/style-rtl.css">

    <link rel="stylesheet" href="~/Content/jquery-steps-master/demo/css/normalize.css">
    <link rel="stylesheet" href="~/Content/jquery-steps-master/demo/css/main.css">
    <link rel="stylesheet" href="~/Content/jquery-steps-master/demo/css/jquery.steps.css">

    <link href="~/Content/SmartWizard-master/dist/css/smart_wizard.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/SmartWizard-master/dist/css/smart_wizard_theme_arrows.css" rel="stylesheet" type="text/css" />




    <!-- END Custom CSS-->
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/assets")
</head>
<body data-open="click" data-menu="vertical-menu" data-col="2-columns" class="vertical-layout vertical-menu 2-columns fixed-navbar">


    <!-- navbar-fixed-top-->
    <nav class="header-navbar navbar navbar-with-menu navbar-fixed-top navbar-semi-dark navbar-shadow">
        <div class="navbar-wrapper">
            <div class="navbar-header" style="background: white;">
                <ul class="nav navbar-nav">
                    <li class="nav-item mobile-menu hidden-md-up float-xs-left"><a class="nav-link nav-menu-main menu-toggle hidden-xs"><i style="color: #000;" class="icon-menu5 font-large-1"></i></a></li>
                    <li style="padding-top: 4px;" class="nav-item">
                        <a style="padding:0" href="" class="navbar-brand nav-link">
                            <object class="logo-object" type="image/svg+xml" data="~/Content/FinalVersion/IBCF.svg"></object>
                            @*<img alt="branding logo" src="~/Content/FinalVersion/IBC-PurpleBlue.png" data-expand="/Content/FinalVersion/IBC-PurpleBlue.png" data-collapse="/Content/FinalVersion/IBC-PurpleBlue.png" class="brand-logo">*@
                        </a>

                        @*<span class="logo-text">الصندوق التعاوني</span>*@
                    </li>
                    <li class="nav-item hidden-md-up float-xs-right"><a data-toggle="collapse" data-target="#navbar-mobile" class="nav-link open-navbar-container"><i style="color: #000;" class="icon-ellipsis pe-2x icon-icon-rotate-right-right"></i></a></li>
                </ul>
            </div>
            <div class="navbar-container content container-fluid">
                <div id="navbar-mobile" class="collapse navbar-toggleable-sm">
                    <ul class="nav navbar-nav">
                        <li class="nav-item hidden-sm-down"><a class="nav-link nav-menu-main menu-toggle hidden-xs"><i class="icon-menu5">         </i></a></li>
                    </ul>
                    <ul class="nav navbar-nav float-xs-right">
                        @Html.Partial("_LoginPartial")
                    </ul>
                </div>
            </div>
        </div>
    </nav>
    <!-- ////////////////////////////////////////////////////////////////////////////-->
    <!-- main menu-->
    <div data-scroll-to-active="true" class="main-menu menu-fixed menu-dark menu-accordion menu-shadow">
        <!-- main menu header-->
        <div class="main-menu-header">
            @*<input type="text" placeholder="Search" class="menu-search form-control round" />*@
        </div>
        <!-- / main menu header-->
        <!-- main menu content-->
        <div class="main-menu-content">
            <ul id="main-menu-navigation" data-menu="menu-navigation" class="navigation navigation-main">
                <li class=" nav-item">
                    <a href="#">
                        <i class="icon-home3"></i>
                        <span data-i18n="nav.dash.main" class="menu-title">الصفحة الرئيسية</span>
                    </a>
                    <ul class="menu-content">
                        <li class="">
                            @Html.ActionLink("الصفحة الرئيسية", "Index", "Home",new { Area = "Public" }, new { })
                        </li>
                    </ul>
                </li>
            </ul>
        </div>
        <!-- /main menu content-->
        <!-- main menu footer-->
        <!-- include includes/menu-footer-->
        <!-- main menu footer-->
    </div>
    <!-- / main menu-->
    @Scripts.Render("~/bundles/sweetalert")
    <div class="app-content content container-fluid">
        <div class="content-wrapper">
            <div class="content-header row">
            </div>
            <div class="content-body">
                <!-- stats -->
                @if (TempData["Failure"] != null)
                {
                    <script>
                        swal("", "@TempData["Failure"].ToString()", "error");
                    </script>
                    @*<div class="alert alert-danger alert-dismissable">
                            <p>@TempData["Failure"].ToString()</p>
                        </div>*@
                }
                else if (TempData["Success"] != null)
                {
                    <script>
                        swal("", "@TempData["Success"].ToString()", "success");
                    </script>
                    @*<div class="alert alert-success alert-dismissable">
                            <p>@TempData["Success"].ToString()</p>
                        </div>*@
                }
                @RenderBody()
            </div>
        </div>
    </div>
    <!-- ////////////////////////////////////////////////////////////////////////////-->
    <!-- BEGIN VENDOR JS-->
    <script src="~/Scripts/core/libraries/jquery.min.js" type="text/javascript"></script>
    <script src="~/Content/assets/js/jquery.unobtrusive-ajax.js" type="text/javascript"></script>
    <script src="~/Scripts/ui/tether.min.js" type="text/javascript"></script>
    <script src="~/Scripts/core/libraries/bootstrap.min.js" type="text/javascript"></script>
    <script src="~/Scripts/ui/perfect-scrollbar.jquery.min.js" type="text/javascript"></script>
    <script src="~/Scripts/ui/unison.min.js" type="text/javascript"></script>
    <script src="~/Scripts/ui/blockUI.min.js" type="text/javascript"></script>
    <script src="~/Scripts/ui/jquery.matchHeight-min.js" type="text/javascript"></script>
    <script src="~/Scripts/ui/screenfull.min.js" type="text/javascript"></script>
    <script src="~/Scripts/bootstrap-datepicker/moment-with-locales.min.js"></script>
    <script src="~/Scripts/bootstrap-datepicker/bootstrap-datetimepicker.min.js"></script>

    @*<script src="~/Content/jquery-steps-master/lib/modernizr-2.6.2.min.js"></script>
        <script src="~/Content/jquery-steps-master/lib/jquery-1.9.1.min.js"></script>
        <script src="~/Content/jquery-steps-master/lib/jquery.cookie-1.3.1.js"></script>
        <script src="~/Content/jquery-steps-master/build/jquery.steps.js"></script>*@


    @*<script type="text/javascript" src="~/Content/SmartWizard-master/dist/js/jquery.smartWizard.min.js"></script>*@
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/Adminjquiry")

   

    <!-- BEGIN VENDOR JS-->
    <!-- BEGIN PAGE VENDOR JS-->
    <script src="~/Scripts/charts/chart.min.js" type="text/javascript"></script>
    <!-- END PAGE VENDOR JS-->
    <!-- BEGIN ROBUST JS-->
    <script src="~/Scripts/core/app-menu.js" type="text/javascript"></script>
    <script src="~/Scripts/core/app.js" type="text/javascript"></script>
    <!-- END ROBUST JS-->
    <!-- BEGIN PAGE LEVEL JS-->
    <!--<script src="~/Scripts/scripts/pages/dashboard-lite.js" type="text/javascript"></script>-->
    <!-- END PAGE LEVEL JS-->
    @RenderSection("Scripts", required: false)

</body>
</html>
